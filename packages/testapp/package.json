{
  "name": "testapp",
  "version": "3.0.9",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "dotenv -e .env.local db migrate $(pwd)/src/database/migrations",
